{
    "chmod": "0755",
    "main": "bin/marathon",
    "output": "marathon",
    "alias": "marathon",
    "force-autodiscovery": true,
    "directories": [
        "src",
        "vendor"
    ],
    "files": [
        "composer.json",
        ".env",
        "config/bundles.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "tests", "test", "Docs", "docs"],
            "in": "src"
        }
    ],
    "banner": "*** Marathon (Version 0.1.8) ***",
    "stub": true,
    "dump-autoload": false,
    "annotations": true
}