{
    "chmod": "0750",
    "main": "bin/marathon",
    "output": "marathon",
    "alias": "marathon",
    "force-autodiscovery": true,
    "directories": [
        "config",
        "src"
    ],
    "directories-bin": ["bin"],
    "files": [
        "composer.json",
        ".env"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "tests", "test", "Docs", "docs"],
            "in": "vendor"
        },
        {
            "name": ["*.yaml", "*.yml", "*.php"],
            "exclude": ["test"],
            "in": "config"
        }
    ],
    "banner": "*** Marathon (Version 0.1.8) ***",
    "stub": true,
    "dump-autoload": false,
    "annotations": false,
    "metadata": [
        "symfony-console"
    ],
    "check-requirements": true
}